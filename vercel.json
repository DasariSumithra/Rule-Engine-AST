{
    "version": 2,
    "builds": [
      {
        "src": "src/frontend/index.html",
        "use": "@vercel/static" 
      },
      {
        "src": "src/app.js",
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/frontend/index.html" 
      }
    ]
  }
  